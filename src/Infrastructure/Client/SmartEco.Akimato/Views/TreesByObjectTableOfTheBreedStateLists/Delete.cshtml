@model SmartEco.Akimato.Models.TreesByObjectTableOfTheBreedStateList
@using SmartEco.Akimato.Controllers
@using Microsoft.Extensions.Localization
@inject IStringLocalizer<SharedResources> SharedLocalizer

@{
